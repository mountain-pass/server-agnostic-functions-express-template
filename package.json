{
    "name": "server-agnostic-functions-express-template",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "ts-mocha --timeout 30000 --bail --recursive './test/**/*.test.ts' './test/*.test.ts' --reporter-option maxDiffSize=8128",
        "test:coverage": "nyc --reporter=html --reporter=text --reporter=text-summary npm test",
        "test:wip": "npm run test -- --grep wip",
        "watch:test": "nodemon --watch test --watch src --exec 'npm run test'",
        "watch:test:coverage": "nodemon --watch test --watch src --exec 'npm run test:coverage'",
        "watch:test:wip": "nodemon --watch test --watch src --exec 'npm run test:wip'",
        "build": "tsc --build tsconfig.production.json",
        "build:clean": "tsc --build --clean",
        "start": "dotenv -e .env.dev ts-node src/index.ts",
        "start:prod": "node dist/index.js",
        "start:watch": "dotenv -e .env.dev nodemon src/index.ts",
        "lint": "eslint --ext .js,.ts src",
        "lint:fix": "eslint --fix --ext .js,.ts src",
        "prettier": "prettier --write 'src/**/*.{js,ts,json}|*.{js,ts,json}'",
        "pretty-quick": "pretty-quick",
        "npm-check": "npm-check --production",
        "precommit": "yarn pretty-quick && yarn npm-check && yarn lint && yarn test:coverage",
        "prepare": "if [[ -x \"$(command -v husky)\" ]]; then husky install; fi"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mountain-pass/server-agnostic-functions-express-template.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mountain-pass/server-agnostic-functions-express-template/issues"
    },
    "homepage": "https://github.com/mountain-pass/server-agnostic-functions-express-template#readme",
    "dependencies": {
        "@mountainpass/server-agnostic-functions-core": "1.0.60",
        "@mountainpass/server-agnostic-functions-express": "1.0.60",
        "express": "4.18.2",
        "mongodb": "5.5.0"
    },
    "devDependencies": {
        "@types/chai": "4.3.5",
        "@types/chai-as-promised": "7.1.5",
        "@types/express": "4.17.17",
        "@types/mocha": "10.0.1",
        "@types/morgan": "1.9.4",
        "@types/node": "20.2.5",
        "@types/serve-static": "1.15.1",
        "@types/sinon": "10.0.15",
